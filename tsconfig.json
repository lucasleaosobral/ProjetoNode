{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "types": ["reflect-metadata"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es5"
    ]
  }, "include": [
    "src/**/*.ts"
  ]
}